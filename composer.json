{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "arhitector/yandex": "dev-master",
        "artesaos/seotools": "^0.22.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "cocur/slugify": "4.2",
        "cviebrock/eloquent-sluggable": "^10.0",
        "doctrine/dbal": "^3.4",
        "dragon-code/contracts": "2.19.1",
        "fakerphp/faker": "^1.20",
        "fedeisas/laravel-mail-css-inliner": "^4.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.4",
        "intervention/image": "^2.7",
        "jenssegers/agent": "^2.6",
        "lab404/laravel-impersonate": "^1.7",
        "larabug/larabug": "^2.5",
        "laravel-lang/publisher": "^12.2",
        "laravel/framework": "^8.0",
        "laravel/sanctum": "^2.15",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.4",
        "laravolt/avatar": "^4.1",
        "league/html-to-markdown": "^5.1",
        "maatwebsite/excel": "^3.1",
        "nicolaslopezj/searchable": "1.*",
        "ohansyah/laravelbenchmark": "^0.1.2",
        "php-http/message-factory": "^1.1",
        "predis/predis": "^2.0",
        "psr/http-factory": "^1.0",
        "rappasoft/laravel-authentication-log": "^1.3",
        "shetabit/visitor": "^3.1",
        "spatie/laravel-collection-macros": "^7.6",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-rate-limited-job-middleware": "^1.5",
        "spatie/pdf-to-image": "^2.2",
        "staudenmeir/eloquent-has-many-deep": "^1.14",
        "tightenco/ziggy": "^1.4",
        "torann/geoip": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "facade/ignition": "^2.3.6",
        "laravel-lang/lang": "^10.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": [
            "php artisan lang:update"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
